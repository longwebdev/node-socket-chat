<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Chat Page</title>
    <script src="jquery.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="xuly.js"></script>
    <link rel="stylesheet" href="layout.css" type="text/css">
</head>

<body>
    <div id="wrapper">

        <div id="loginForm">
            <h3>WHAT IS YOUR NAME?</h3>
            <input type="text" id="txtusername">
            <input type="button" value="Register" id="btnRegister">
        </div>

        <div id="chatForm">
            <div id="left">
                <div id="boxTitle">Users Online</div>
                <div id="boxContent">
                    
                    
                </div>
            </div>

            <div id="right">

                <div id="sayHi">Hello <span id="currentUser">Teo</span>,
                    <input type="button" id="btnLogout" value="LogOut">
                </div>
                <div id="listMessage">

                </div>
                <div id="thongbao"></div>
                <div id="message">
                    <input type="text" id="txtMessage">
                    <input type="button" value="Send" id="btnSendMessage">
                </div>
                
            </div>

        </div>

    </div>
</body>

</html>